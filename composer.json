{
    "name": "pi/pi",
    "description": "Pi Engine is a role oriented application development engine with support for multitenancy SaaS and SSO/OAuth, developed upon Zend Framework, jQuery, Bootstrap, Angular.",
    "type": "composer",
    "keywords": [
        "pi",
        "zendframework",
        "angular",
        "bootstrap",
        "jquery"
    ],
    "homepage": "http://pialog.org/",
    "license": "New BSD",
    "require": {
        "php": ">=5.3.23"
    },
    "require-dev": {
    },
    "suggest": {
        "ext-intl": "ext/intl for i18n features",
        "ext-curl": "ext/curl for remote services"
    },
    "autoload": {
        "psr-0": {
            "Pi\\": "lib/",
            "Zend\\": "lib/",
            "GeoIp2\\": "vendor/",
            "Imagine\\": "vendor/",
            "MaxMind\\": "vendor/",
            "RandomLib\\": "vendor/",
            "SecurityLib\\": "vendor/"
        }
    },
    "replace": {
   }
}
    "type": "project",
    "keywords": [
        "Pi",
        "ZendFramework",
        "Multi-Tenant",
        "SaaS",
        "PHP"
    ],
    "homepage": "http://pialog.org",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Taiwen Jiang",
            "email": "taiwen@me.com"
        },
        {
            "name": "Pi Team",
            "homepage": "https://github.com/pi-engine/pi/graphs/contributors"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "ext-ctype": "*",
        "ext-fileinfo": "*",
        "ext-gd": "*",
        "ext-mbstring": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-spl": "*",
        "lib-curl": "*"
